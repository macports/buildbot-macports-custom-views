{
  "name": "buildbot-macports-custom-views",
  "version": "0.1.2",
  "description": "MacPorts Custom Views Plugin for Buildbot",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "watch": "webpack --watch --mode development",
    "test": "jest",
    "lint": "node_modules/.bin/eslint --ext .js,.vue src",
    "lint:fix": "node_modules/.bin/eslint --ext .js,.vue src --fix"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{vue,}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/macports/buildbot-macports-custom-views.git"
  },
  "keywords": [
    "Buildbot",
    "MacPorts",
    "Vue"
  ],
  "author": "Rajdeep Bharati",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/macports/buildbot-macports-custom-views/issues"
  },
  "homepage": "https://github.com/macports/buildbot-macports-custom-views#readme",
  "dependencies": {
    "@babel/core": "^7.12.9",
    "@hapi/cryptiles": "^5.0.0",
    "@hapi/hoek": "^9.0.4",
    "ajv": "^6.11.0",
    "autolinker": "^3.13.0",
    "axios": "^0.19.2",
    "lodash": "^4.17.15",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.2",
    "vue-loader": "^15.9.0",
    "vue-template-compiler": "^2.6.11",
    "vuejs-datepicker": "^1.6.2",
    "vuejs-uib-pagination": "^1.0.6"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.8.4",
    "@vue/test-utils": "^1.0.0-beta.31",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "bitbar-webpack-progress-plugin": "^1.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "expect": "^25.1.0",
    "file-loader": "^4.0.0",
    "jest": "^24.9.0",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "json2ts": "0.0.7",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "typescript": "^3.7.5",
    "vue-jest": "^4.0.0-beta.2",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  },
  "resolutions": {
    "serialize-javascript": ">=2.1.1"
  }
}
